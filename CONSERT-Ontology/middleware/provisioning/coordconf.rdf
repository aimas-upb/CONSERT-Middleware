    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >hasAcquisitionType</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="hasSpecificTTLSpec">
