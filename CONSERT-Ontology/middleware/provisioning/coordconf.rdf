    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >nrSuccessfulQueries</rdfs:label>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="#isDerivedAssertion">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Property of an AssertionSpecificStatistic denoting whether the ContextAssertion for which the statistic is computed is a derived one or not.</rdfs:comment>
    <rdfs:domain rdf:resource="#AssertionSpecificStatistic"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >isDerivedAssertion</rdfs:label>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="nrSuccessfulDerivations">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Property of an AssertionSpecificInferenceStatistic instance stating the number of successful inference calls made for the given ContextAssertion within the time frame specified by the run_window parameter.</rdfs:comment>
    <rdfs:domain rdf:resource="#AssertionSpecificStatistic"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
