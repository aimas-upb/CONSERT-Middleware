  <owl:ObjectProperty rdf:ID="hasCommandRule">
    <rdfs:range>
      <spin:ConstructTemplate rdf:ID="CommandRuleTemplate">
        <spin:labelTemplate rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
