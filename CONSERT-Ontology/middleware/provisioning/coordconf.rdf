    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >setsUpdateMode</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#hasDefaultIntegrityConstraintResolution">
    <rdfs:domain rdf:resource="#ControlPolicy"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >hasDefaultIntegrityConstraintResolution</rdfs:label>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
