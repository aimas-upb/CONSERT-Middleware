    >assignedOrgManager</rdfs:label>
    <rdfs:range rdf:resource="#AgentAddress"/>
    <rdfs:domain>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <owl:Class rdf:about="#CtxCoordSpec"/>
          <owl:Class rdf:about="#CtxSensorSpec"/>
          <owl:Class rdf:about="#CtxUserSpec"/>
          <owl:Class rdf:about="#CtxQueryHandlerSpec"/>
        </owl:unionOf>
      </owl:Class>
    </rdfs:domain>
  </owl:ObjectProperty>
